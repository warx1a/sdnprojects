package com.snoopdogg.grinder;

public enum Grind {

	CHOCOLATE(1973,"Chocolate bar"),
	BLUE_DRAG_SCALE(243,"Blue dragon scale");
	
	private int id;
	private String title;
	
	Grind(final int id,String title) {
		this.id = id;
		this.title = title;
	}
	
	public int getId() {
		return id;
	}
	
	public String name() {
		return title;
	}
}
