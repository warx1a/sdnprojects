package dungeoneering.wrappers;

import dungeoneering.enums.DoorState;

public class Door {
	
	private final DoorState state;
	private final Integer id;
	
	public Door(DoorState state,Integer id) {
		this.state = state;
		this.id = id;
	}
	
	public int getId() {
		return id;
	}
	
	public DoorState getState() {
		return state;
	}
	
}
